{erl_opts, [debug_info]}.
{deps, [
	{jiffy, {git, "https://github.com/davisp/jiffy.git", {ref, "a7db72493d2288533afbb417d5c2e6f11db84639"}}},
	{forms, {git, "https://github.com/efcasado/forms.git", {ref, "491b6768dd9d4f2cd22a90327041b630f68dd66a"}}}
]}.
{overrides, [
	{override, jiffy, [
		{plugins, [pc]},
		{artifacts, ["priv/jiffy.so"]},
		{provider_hooks, [
			{post, [
				{compile, {pc, compile}},
				{clean, {pc, clean}}
			]}
		]}
	]}
]}.